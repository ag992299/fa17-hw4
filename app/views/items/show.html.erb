<p id="notice"><%= notice %></p>
<h2> Here are all my user, kitties, and tasks below ^_^ </h2>
<table>
<% User.all.each do |user|%>
	<tr>
		<td> <%= user.name %> </td>
		<td> <%= user.email %> </td>
		<td> <%= user.age %> </td>
	</tr>
<% end %>
</table>

<table>
<% Cat.all.each do |cat|%>
	<tr>
		<td> <%= cat.name %> </td>
	</tr>
<% end %>


</table>
<% Todo.all.each do |todo|%>
	<tr>
		<td> <%= todo.tasks %> </td>
		<td> <%= todo.finished %> </td>
	</tr>
<% end %>
</table>

